(if (nil? (resolve 'n)) (set n 10000000))

(set t (table [OrderId Symbol Price Size Tape Timestamp] 
    (list (take n (guid 1000000))
          (take n [AAPL GOOG MSFT IBM AMZN FB BABA]) 
          (as 'F64 (til n)) 
          (take n (+ 1 (til 3)))
          (map (fn [x] (as 'C8 x)) (take n (til 10)))
          (as 'TIMESTAMP (til n)) 
    ))
)